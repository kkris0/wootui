{
  "name": "woodottranslate",
  "module": "src/index.tsx",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.tsx",
    "build": "bun build ./src/index.tsx --outdir dist",
    "build:macos-arm64": "bun build --compile ./src/index.tsx --target=bun-darwin-arm64 --outfile dist/app-macos-arm64",
    "build:macos-x64": "bun build --compile ./src/index.tsx --target=bun-darwin-x64 --outfile dist/app-macos-x64",
    "build:windows": "bun build --compile ./src/index.tsx --target=bun-windows-x64 --outfile dist/app-windows.exe",
    "build:linux-x64": "bun build --compile ./src/index.tsx --target=bun-linux-x64 --outfile dist/app-linux-x64",
    "build:linux-arm64": "bun build --compile ./src/index.tsx --target=bun-linux-arm64 --outfile dist/app-linux-arm64"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dotenv": "^17.2.3",
    "react-devtools-core": "7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.1",
    "@biomejs/biome": "^2.3.8",
    "@google/genai": "^1.34.0",
    "@opentui-ui/toast": "^0.0.4",
    "@opentui/core": "^0.1.63",
    "@opentui/react": "^0.1.63",
    "@toon-format/toon": "^2.1.0",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.7",
    "ai": "^6.0.3",
    "conf": "^15.0.2",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "slugify": "^1.6.6",
    "zod": "^4.2.1"
  }
}